package com.gosun.solr.query.result;

/**
 * 构面查询 单个构面元素
 * @author cxp
 * 
 */
public class FacetElementQueryResult {
	/**
	 * 构面元素的值
	 */
	private String value;
	
	/**
	 * 构面元素的数量
	 */
	private long count;

	public String getValue() {
		return value;
	}

	public FacetElementQueryResult setValue(String value) {
		this.value = value;
		return this;
	}

	public long getCount() {
		return count;
	}

	public FacetElementQueryResult setCount(long count) {
		this.count = count;
		return this;
	}

	@Override
	public String toString() {
		return "FacetElementQueryResult [value=" + value + ", count=" + count + "]";
	}
}
